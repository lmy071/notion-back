    "debug": "~2.6.9",
    "dotenv": "^17.2.3",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "multer": "^2.0.2",
    "mysql2": "^3.16.2",
    "node-cron": "^4.2.1",
    "pinyin-pro": "^3.28.0",
    "pug": "2.0.0-beta11"
  }
